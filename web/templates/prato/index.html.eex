<h1>Card√°pio</h1>

<table class="table">
<ul class="flex-container" >
<%= for prato <- @pratos do %>



        <ul class="list-grid grid-view"  height:10px;>




          <li class="item">
            <figure class="item-pic"> <img
                  src="/images/yellow-restaurant-64.png"></figure>
            <h3 class="item-title list-cell"><%= prato.nome %></h3>
              <h3 class="item-title list-cell">R$ <%= prato.valor %></h3>

            <%= link "Mais", to: restaurante_prato_path(@conn, :show, @restaurante_id, prato), class: "btn btn-default btn-xs" %>
            <%= link "Editar", to: restaurante_prato_path(@conn, :edit, @restaurante_id, prato), class: "btn btn-default btn-xs" %>
            <%= link "Apagar", to: restaurante_prato_path(@conn, :delete, @restaurante_id, prato), method: :delete, data: [confirm: "Are you sure?"], class: "btn btn-danger btn-xs" %>
          </li>

        </ul>





<% end %>
</ul>
<ul></ul>
 <button class="plus1"><%= link "   +", to: restaurante_prato_path(@conn, :new, @restaurante_id)%></button>


